{
  "name": "@vncsleal/saw",
  "version": "0.2.3",
  "description": "SAW: minimal library + CLI for signed canonical JSON feeds, llms.txt, verification, and anti-scrape HTML.",
  "bin": { "saw": "dist/index.js" },
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vncsleal/saw.git"
  },
  "bugs": {
    "url": "https://github.com/vncsleal/saw/issues"
  },
  "homepage": "https://github.com/vncsleal/saw#readme",
  "keywords": ["saw","canonical","ed25519","feed","verification","llms.txt","antiscrape","signature"],
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
  "build": "tsc -p tsconfig.json",
  "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "tweetnacl": "^1.0.3",
    "zod": "^3.23.0"
  },
  "engines": { "node": ">=18" },
  "publishConfig": {
    "access": "public"
  }
}
